n                lines = [l.strip() for l in s.split('\\\\n') if '|' in l and '---' not in l]\\n                if len(lines) > 1:\\n                    header = [h.strip() for h in lines[0].split('|') if h.strip()]\\n                    data_rows = []\\n                    for line in lines[1:]:\\n                        values = [v.strip() for v in line.split('|') if v.strip()]\\n                        if len(values) == len(header):\\n                            data_rows.append(dict(zip(header, values)))\\n                    \\n                    scores = {}\\n                    for row in data_rows:\\n                        dim = row.get('Dimension')\\n                        if dim:\\n                           scores[dim] = {\\n                               'raw_score': float(row.get('Raw Score', 0.0)),\\n                               'salience': float(row.get('Salience', 0.0)),\\n                               'confidence': float(row.get('Confidence', 0.0))\\n                           }\\n                    return scores\\n            except Exception:\\n                return None\\n        return None\\n\\n    grouped_artifacts = {}\\n    for artifact in data:\\n        analysis_id = artifact.get('analysis_id')\\n        if not analysis_id:\\n            continue\\n        \\n        if analysis_id not in grouped_artifacts:\\n            grouped_artifacts[analysis_id] = {}\\n\\n        if artifact['step'] == 'score_extraction':\\n            scores = clean_json_string(artifact.get('scores_extraction', '{}'))\\n            if scores:\\n                grouped_artifacts[analysis_id]['dimensional_scores'] = scores\\n        \\n        elif artifact['step'] == 'derived_metrics_generation':\\n            metrics = clean_json_string(artifact.get('derived_metrics', '{}'))\\n            if metrics:\\n                # Standardize inconsistent derived metrics structure\\n                if 'tension_indices' in metrics and 'cohesion_indices' in metrics:\\n                    flat_metrics = {**metrics['tension_indices'], **metrics['cohesion_indices']}\\n                    flat_metrics['strategic_contradiction_index'] = metrics.get('strategic_contradiction_index')\\n                    grouped_artifacts[analysis_id]['derived_metrics'] = flat_metrics\\n                else: # Already flat or semi-flat\\n                    grouped_artifacts[analysis_id]['derived_metrics'] = metrics\\n    return grouped_artifacts\\n\\ndef _create_dataframe(data: List[Dict[str, Any]], corpus_manifest: Dict[str, Any]) -> Optional[pd.DataFrame]:\\n    \\\"\\\"\\\"\\n    Creates a clean pandas DataFrame from parsed artifacts and corpus manifest.\\n    This is the core data preparation step.\\n\\n    Args:\\n        data: The raw list of analysis artifacts.\\n        corpus_manifest: The dictionary representation of the corpus manifest.\\n\\n    Returns:\\n        A pandas DataFrame ready for analysis, or None if creation fails.\\n    \\\"\\\"\\\"\\n    try:\\n        parsed_data = _parse_artifacts(data)\\n        \\n        # Assume order of unique analysis IDs corresponds to manifest document order\\n        unique_analysis_ids = sorted(list(parsed_data.keys()))\\n        doc_metadata = corpus_manifest['documents']\\n\\n        if len(unique_analysis_ids) != len(doc_metadata):\\n            # Log an error or raise an exception\\n            print(f\\\"Error: Mismatch between artifacts ({len(unique_analysis_ids)}) and manifest docs ({len(doc_metadata)})\\\")\\n            # This is a fallback to process what we can match\\n            num_docs = min(len(unique_analysis_ids), len(doc_metadata))\\n            unique_analysis_ids = unique_analysis_ids[:num_docs]\\n            doc_metadata = doc_metadata[:num_docs]\\n        \\n        records = []\\n        for i, analysis_id in enumerate(unique_analysis_ids):\\n            doc_id = doc_metadata[i]['document_id']\\n            artifact_data = parsed_data[analysis_id]\\n            \\n            record = {'document_id': doc_id}\\n            record.update(doc_metadata[i]['metadata'])\\n            \\n            # Flatten dimensional scores\\n            if 'dimensional_scores' in artifact_data:\\n                for dim, scores in artifact_data['dimensional_scores'].items():\\n                    record[f'{dim}_raw_score'] = scores.get('raw_score')\\n                    record[f'{dim}_salience'] = scores.get('salience')\\n            \\n            # Flatten derived metrics\\n            if 'derived_metrics' in artifact_data:\\n                for metric, value in artifact_data['derived_metrics'].items():\\n                    record[metric] = value\\n            \\n            records.append(record)\\n            \\n        df = pd.DataFrame(records)\\n        \\n        # Convert numeric columns, coercing errors\\n        for col in df.columns:\\n            if '_score' in col or '_salience' in col or '_index' in col or '_tension' in col:\\n                df[col] = pd.to_numeric(df[col], errors='coerce')\\n        \\n        return df\\n\\n    except Exception as e:\\n        print(f\\\"Error in _create_dataframe: {e}\\\")\\n        return None\\n\\n# --- Statistical Analysis Functions (Tier 3) ---\\n\\ndef calculate_descriptive_statistics(df: pd.DataFrame) -> Optional[Dict[str, Any]]:\\n    \\\"\\\"\\\"\\n    Calculates descriptive statistics (mean, std, min, max) for key metrics.\\n    TIER 3: Core exploratory function.\\n\\n    Args:\\n        df: The analysis DataFrame.\\n\\n    Returns:\\n        A dictionary of descriptive statistics.\\n    \\\"\\\"\\\"\\n    if df is None:\\n        return None\\n    try:\\n        metrics_to_describe = [\\n            'tribal_dominance_raw_score', 'individual_dignity_raw_score',\\n            'fear_raw_score', 'hope_raw_score',\\n            'envy_raw_score', 'mudita_raw_score',\\n            'enmity_raw_score', 'amity_raw_score',\\n            'fragmentative_goals_raw_score', 'cohesive_goals_raw_score',\\n            'strategic_contradiction_index',\\n            'descriptive_cohesion_index', 'motivational_cohesion_index', 'full_cohesion_index'\\n        ]\\n        # Filter out columns that may not exist if parsing failed for some\\n        existing_metrics = [m for m in metrics_to_describe if m in df.columns]\\n        desc_stats = df[existing_metrics].describe().to_dict()\\n        return desc_stats\\n    except Exception as e:\\n        return {'error': str(e)}\\n\\ndef perform_correlation_analysis(df: pd.DataFrame) -> Optional[Dict[str, Any]]:\\n    \\\"\\\"\\\"\\n    Performs correlation analysis on key CFF metrics.\\n    TIER 3: Exploratory only. P-values should be interpreted with caution.\\n\\n    Args:\\n        df: The analysis DataFrame.\\n\\n    Returns:\\n        A dictionary containing the correlation matrix.\\n    \\\"\\\"\\\"\\n    if df is None:\\n        return None\\n    try:\\n        metrics_to_correlate = [\\n            'tribal_dominance_raw_score', 'fear_raw_score', 'envy_raw_score',\\n            'enmity_raw_score', 'fragmentative_goals_raw_score',\\n            'individual_dignity_raw_score', 'hope_raw_score', 'mudita_raw_score',\\n            'amity_raw_score', 'cohesive_goals_raw_score',\\n            'strategic_contradiction_index', 'full_cohesion_index'\\n        ]\\n        existing_metrics = [m for m in metrics_to_correlate if m in df.columns]\\n        corr_matrix = df[existing_metrics].corr(method='pearson')\\n        return corr_matrix.to_dict('index')\\n    except Exception as e:\\n        return {'error': str(e)}\\n\\ndef analyze_group_differences(df: pd.DataFrame, group_by_col: str) -> Optional[Dict[str, Any]]:\\n    \\\"\\\"\\\"\\n    Calculates descriptive statistics for key metrics, grouped by a specified column.\\n    This is the primary method for comparing groups in a Tier 3 analysis.\\n\\n    Args:\\n        df: The analysis DataFrame.\\n        group_by_col: The column to group by (e.g., 'career_phase').\\n\\n    Returns:\\n        A dictionary of grouped statistics.\\n    \\\"\\\"\\\"\\n    if df is None or group_by_col not in df.columns:\\n        return None\\n    try:\\n        metrics_to_analyze = [\\n            'tribal_dominance_raw_score', 'enmity_raw_score',\\n            'strategic_contradiction_index', 'full_cohesion_index'\\n        ]\\n        existing_metrics = [m for m in metrics_to_analyze if m in df.columns]\\n        \\n        # Use describe() for a richer summary\\n        grouped_stats = df.groupby(group_by_col)[existing_metrics].describe()\\n        \\n        # Convert the MultiIndex DataFrame to a nested dictionary for JSON serialization\\n        results = {}\\n        for group, group_df in grouped_stats.iterrows():\\n            group_name = group[0]\\n            metric_name = group[1]\\n            if group_name not in results:\\n                results[group_name] = {}\\n            results[group_name][metric_name] = group_df.to_dict()\\n            \\n        return results\\n    except Exception as e:\\n        return {'error': str(e)}\\n\\ndef calculate_internal_consistency(df: pd.DataFrame) -> Optional[Dict[str, Any]]:\\n    \\\"\\\"\\\"\\n    Calculates Cronbach's alpha to assess the internal consistency of CFF subscales.\\n    TIER 3: Provides a reliability check for the framework on this specific corpus.\\n\\n    Args:\\n        df: The analysis DataFrame.\\n\\n    Returns:\\n        A dictionary with Cronbach's alpha values for cohesive and fragmentative scales.\\n    \\\"\\\"\\\"\\n    if df is None:\\n        return None\\n    try:\\n        fragmentative_dims = [\\n            'tribal_dominance_raw_score', 'fear_raw_score', 'envy_raw_score',\\n            'enmity_raw_score', 'fragmentative_goals_raw_score'\\n        ]\\n        cohesive_dims = [\\n            'individual_dignity_raw_score', 'hope_raw_score', 'mudita_raw_score',\\n            'amity_raw_score', 'cohesive_goals_raw_score'\\n        ]\\n\\n        # Filter to only existing columns\\n        fragmentative_dims_exist = [d for d in fragmentative_dims if d in df.columns]\\n        cohesive_dims_exist = [d for d in cohesive_dims if d in df.columns]\\n\\n        # Drop rows with any NaN in the selected dimensions for alpha calculation\\n        alpha_fragmentative = pg.cronbach_alpha(data=df[fragmentative_dims_exist].dropna())\\n        alpha_cohesive = pg.cronbach_alpha(data=df[cohesive_dims_exist].dropna())\\n\\n        return {\\n            'fragmentative_scale_alpha': alpha_fragmentative[0],\\n            'fragmentative_scale_ci95': list(alpha_fragmentative[1]),\\n            'cohesive_scale_alpha': alpha_cohesive[0],\\n            'cohesive_scale_ci95': list(alpha_cohesive[1])\\n        }\\n    except Exception as e:\\n        return {'error': str(e)}\\n\\ndef test_hypothesis_h3(df: pd.DataFrame) -> Optional[Dict[str, Any]]:\\n    \\\"\\\"\\\"\\n    Performs a descriptive comparison to evaluate Hypothesis H3.\\n    H3: Higher tribal dominance/enmity for college students vs. conservative activists.\\n\\n    Args:\\n        df: The analysis DataFrame.\\n\\n    Returns:\\n        A dictionary comparing the mean scores for the two groups.\\n    \\\"\\\"\\\"\\n    if df is None:\\n        return None\\n    try:\\n        target_audiences = ['college_students', 'conservative_activists']\\n        filtered_df = df[df['audience'].isin(target_audiences)]\\n        \\n        if filtered_df.empty:\\n            return {'message': 'No data for target audiences found.'}\\n\\n        comparison = filtered_df.groupby('audience')[['tribal_dominance_raw_score', 'enmity_raw_score']].mean()\\n        return comparison.to_dict('index')\\n    except Exception as e:\\n        return {'error': str(e)}\\n\\n# --- Master Execution Function ---\\n\\ndef perform_statistical_analysis(data: List[Dict[str, Any]], corpus_manifest_str: str) -> Dict[str, Any]:\\n    \\\"\\\"\\\"\\n    Master function to orchestrate and execute all statistical analyses.\\n    \\n    Args:\\n        data: The raw list of 28 analysis artifacts.\\n        corpus_manifest_str: The string content of the corpus manifest YAML.\\n        \\n    Returns:\\n        A dictionary containing all statistical results.\\n    \\\"\\\"\\\"\\n    # A simple YAML parser for the manifest\\n    def parse_yaml_like(content):\\n        import yaml\\n        try:\\n            return yaml.safe_load(content)\\n        except yaml.YAMLError:\\n            # Fallback for simple key: value parsing if full YAML fails\\n            data = {}\\n            for line in content.split('\\\\n'):\\n                if ':' in line:\\n                    key, val = line.split(':', 1)\\n                    data[key.strip()] = val.strip()\\n            return data\\n\\n    corpus_manifest = parse_yaml_like(corpus_manifest_str[corpus_manifest_str.find('documents:'):])\\n    df = _create_dataframe(data, corpus_manifest)\\n\\n    if df is None:\\n        return {'error': 'DataFrame creation failed. Cannot proceed with analysis.'}\\n    \\n    # Execute all analyses\\n    results = {}\\n    results['descriptive_statistics'] = calculate_descriptive_statistics(df)\\n    results['correlation_analysis'] = perform_correlation_analysis(df)\\n    results['reliability_analysis'] = calculate_internal_consistency(df)\\n    \\n    results['group_analysis'] = {\\n        'by_career_phase': analyze_group_differences(df, 'political_phase'),\\n        'by_event_type': analyze_group_differences(df, 'event_type'),\\n        'by_audience': analyze_group_differences(df, 'audience')\\n    }\\n    \\n    results['hypothesis_testing'] = {\\n        'H3_audience_comparison': test_hypothesis_h3(df)\\n    }\\n    \\n    return results\\n\",\n  \"execution_results\": {\n    \"descriptive_statistics\": {\n      \"tribal_dominance_raw_score\": {\n        \"count\": 14.0,\n        \"mean\": 0.825,\n        \"std\": 0.0841505191147517,\n        \"min\": 0.7,\n        \"25%\": 0.8,\n        \"50%\": 0.85,\n        \"75%\": 0.9,